{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.86.24:3030/v1"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development",
      "autoIncrement": true
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://platform.getelevenapp.com/v1",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_IOS": "$REVENUECAT_API_KEY_IOS",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID": "$REVENUECAT_API_KEY_ANDROID"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "preview",
      "autoIncrement": true
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://platform.getelevenapp.com/v1",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_IOS": "$REVENUECAT_API_KEY_IOS",
        "EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID": "$REVENUECAT_API_KEY_ANDROID"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
